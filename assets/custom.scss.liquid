#shopify-section-header {
  header.site-header.items-hidden {
    padding-left: 55px!important;
    @media screen and (max-width: 768px) {
      padding-left: 0px!important;
    }
    .logo-align--center {
      margin: 0 auto;
      display: block;
    }
    .site-header__menu, .site-header__search-toggle {
      display: none!important;
    }
  }
  .site-header {
	padding-left: 85px;
    .site-header__icons {
    position: absolute;
    right: 22px;
    top: -14px;
    width: auto;
    height: 100%;
    align-items: center;
    display: flex;
      @media screen and (max-width: 768px) {
        top: unset;
      }
      @media screen and (max-width: 767px) {
        top: -6px;
      }
      img {
        max-width: unset;
        cursor: pointer;
      }
      .search-form {
        @media screen and (max-width: 767px) {
          display: none;
        }
        .search-form__input {
          border: unset;
    	  background-color: transparent;
          border-bottom: 1px solid #242D2D;
          border-radius: 0px;
          padding: 0px;
          min-height: 25px;
          width: 135px;
          font-family: 'Tungsten';
          text-transform: uppercase;
          letter-spacing: 0.3px;
          font-size: 14px;
          outline: none!important;
          @media screen and (max-width: 768px) {
            width: 90px; 
          }
        }
        .search-bar__submit {
          display: none;
        }
      }
    }
    .site-header__search-toggle {
        display: none;
        position: absolute;
        top: -20px;
        left: 52px;
      	z-index: 9;
        @media screen and (max-width: 768px) {
          display: block;
        }
        svg {
          width: 22px;
    	  height: 20px;
        }
      }
  }
  .logo-align--center {
    left: 15px;
  }
  .site-header__logo {
    img {
      max-width: 550px!important;
      
    }
  }
  .site-header.logo--center {
    padding-top: 40px;
    padding-bottom: 20px;
    padding-left: 0!important;
    padding-right: 0!important;
    @media screen and (max-width: 767px) {
          padding-bottom: 0px;
    }
  }
  .site-header__account, .site-header__cart {
    padding: 0px 11px;
  }
  .site-nav {
  	text-align: left;
    li {
      position: relative;
      display: block;
      margin-bottom: 3vh;
      width: 210px;
    }
    
  }
  .site-hamburger {
  	position: absolute;
    z-index: 9999;
    width: 31px;
    height: 16px;
    left: 10px;
    top: -16px;
    cursor: pointer;
    span {
    	position: relative;
        display: block;
        width: 100%;
        height: 3px;
        top: 0;
        left: 0;
        background-color: #000;
      	transition: all 0.3s ease-in-out;
        &:nth-child(1) {
          top: 0px!important;
        }
        &:nth-child(2) {
          top: 3px;
          transition: all 0.1s ease-in-out;
        }
        &:nth-child(3) {
        	top: 6px;
        }
    }
  }
  .site-hamburger.active {
    span {
      &:nth-child(2) {
        opacity: 0;
      }
      &:nth-child(1) {
        transform: rotate(45deg);
        top: 1px;
      }
      &:nth-child(3) {
        transform: rotate(-45deg);
        top: -4px;
      }
    }
    &:before {
      content: '';
      position: fixed;
      background: #F4F0EC;
      width: 100%;
      height: 0px;
      left: 0;
      top: 0;
    }
  }
  
  #AccessibleNav {
  	position: fixed;
    width: 100%;
    height: 100vh;
    overflow: auto;
    top: 0;
    left: 0;
    z-index: 999;
    background-color: rgba(244, 240, 236,1);
    padding-left: 95px;
    padding-top: 120px;
    transition: all 0.3s ease-in-out;
    opacity: 0;
    visibility: hidden;
    .menuLogo {
      position: absolute;
      bottom: 95px;
      left: 95px;
      transform: translateY(95px);
      transition: all 0.3s ease-in-out;
    }
    >.site-nav{
      opacity: 0;
      visibility: hidden;
      transition: all 0.8s ease-in-out;
    }
    >.site-nav.active{
      opacity: 1;
      visibility: visible;
    }
    
  }
  #AccessibleNav.menuActive {
    opacity: 1;
    visibility: visible;
    .menuLogo {
      transform: translateY(0px);
    }
    #SiteNav {
    	transform: translateY(0px);
    }
  }
  #SiteNav {
    transform: translateY(-95px);
    transition: all 0.3s ease-in-out;
    * {
      font-family: 'Tungsten';
      font-size: 25px;
      font-weight: 400;
    }
    .site-nav__link--active .site-nav__label {
      border: unset;
    }
    .site-nav__label {
      text-transform: uppercase;
    }
    .site-nav__dropdown {
      display: block!important;
      top: 0px!important;
      left: 210px;
      width: auto;
      border: unset;
      transform: translateX(-30px);
      opacity: 0;
      visibility: hidden;
      transition: all 1s ease-in-out;
      .site-nav__link {
        padding: 0px;
      }
    }
    .site-nav__link {
      padding: 0px;
      svg {
        display: none;
      }
    }
    .site-nav__childlist {
      display: block;
      padding: 0px;
    }
    .site-nav__childlist-grid {
      display: block;
    }
    .site-nav__child-link--parent {
      margin: 0px;
    }
    .site-nav__childlist-item {
      
      ul {
        position: absolute;
        top: 0px;
        left: 210px;
        transform: translateX(-30px);
        opacity: 0;
        visibility: hidden;
      }
      &:hover {
        > .site-nav__link > .site-nav__label {
          font-weight: 600;
          color: #E95A29;
        }
        ul {
          opacity: 1;
          visibility: visible;
          transform: translateX(0px);
        }
      }
    }
    li:hover {
      > button > .site-nav__label, >.site-nav__link > .site-nav__label {
        font-weight: 600;
        color: #E95A29;
      }
      .site-nav__dropdown {
        opacity: 1;
        visibility: visible;
        transform: translateX(0px);
      }
    }
    
  }
}
