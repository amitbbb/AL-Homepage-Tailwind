{% javascript %}
 
$(document).ready( function() {
    $(".new-home-blog-section .view-all-btn").on("click", function(e) {
      e.preventDefault();
        $("#blogs-content .blog-list-view").load("/blogs/stories/ .blog-list-view.blog-list.hidden .home-blog");
       setTimeout( function(){
        $('.view-all-btn').addClass('hide-this'); 
       }, 2000); 
    });
});

{% endjavascript %}