{% stylesheet %}
.new-home-blog-section {
  padding: 0 80px;
}
.new-home-blog-section .grid__item .article__grid-image-container{
  margin-bottom: 40px;
}
.new-home-blog-section .view-all-btn {
  height: 75px;
  background: #242d2d;
  color: #fff;
  font-family: Tungsten;
  font-style: normal;
  font-weight: 326;
  text-transform: uppercase;
  margin: 80px auto 0;
  width: 100%;
  max-width: 196px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32px;
  line-height: 50px;
}
.new-home-blog-section .view-all-btn:hover{
  color:#fff;
}
.new-home-blog-section .main-hed-blog {
  margin: 35px 0 !important;
  text-align: right;
  margin-top: 80px!important;
}
.new-home-blog-section .main-hed-blog a {
  font-family: "Compacta";
  font-style: normal;
  font-weight: 400;
  font-size: 45px;
  line-height: 62.1px;
  text-transform: uppercase;
  letter-spacing: normal; 
}
.new-home-blog-section .blog-list-view .first-grid .page-width {
  padding: 0;
  max-width: unset;
}
.new-home-blog-section .blog-list-view .first-grid .page-width .grid {
  margin: 0;
}
.new-home-blog-section
  .blog-list-view
  .first-grid
  .page-width
  .grid
  .grid__item {
  width: 100%;
  left: unset;
  padding: 0;
}
.new-home-blog-section
  .blog-list-view
  .first-grid
  .page-width
  .grid
  .grid__item
  .article-listing {
  padding-top: 0;
  margin-bottom: 0px;
}
.new-home-blog-section
  .blog-list-view
  .first-grid
  .page-width
  .grid
  .grid__item
  .article__title *{
  /*display: none;*/
}
.new-home-blog-section
  .blog-list-view
  .first-grid
  .page-width
  .grid
  .grid__item
  .article__title .blog-title{
  /*display: block;
  position: absolute;
  bottom: 150px;
  left: 50%;
  transform: translateX(-50%);
  font-family: 'Compacta';
  font-size: 85px;
  color: #FFF;*/
}
.new-home-blog-section
  .blog-list-view
  .first-grid
  .page-width
  .grid
  .grid__item
  .blog-list-image
  img {
  position: unset;
  display: block;
}
.new-home-blog-section .blog-list-view .other-list-wrap {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.new-home-blog-section .blog-list-view .other-list-wrap .other-grid {
  width: calc(50% - 25px);
  margin-bottom: 70px; 
}
.other-grid .page-width {
  padding: 0;
  max-width: unset; 
}
.other-grid .page-width .grid {
  margin-left: 0px;
}
.other-grid .page-width .grid .grid__item {
  padding: 0;
  width: 100%;
  left: 0;
}
.other-grid .page-width .grid .grid__item .article-listing {
  padding: 0;
  margin: 0;
}
.other-grid
  .page-width
  .grid
  .grid__item
  .article-listing
  .blog-list-image
  img {
  position: unset;
  height: 48vh;
  object-fit: cover;
  display: block;
}
.another-blog-list .article__title {
  margin-top: 36px; 
  margin-bottom: 36px; 
}  
.another-blog-list .article__title > a, .new-home-blog-section .grid--blog .article__title .story-title {
  font-family: "Compacta";
  font-style: normal;
  font-weight: bold;
  font-size: 32px;
  line-height: 38px;
  text-transform: uppercase;
} 
.another-blog-list .article__title .edi, .new-home-blog-section .grid--blog .article__title .edi {
  font-family: "Kepler";
  font-style: normal;
  font-weight: 500;
  font-size: 22px;
  line-height: 26px;
  text-transform: capitalize;
  margin: 0;
  margin-top: 20px;
}
.another-blog-list .article__title .read-story, .new-home-blog-section .grid--blog .article__title .read-story {
  margin: 0;
  margin-top: 20px;
}
.another-blog-list .article__title .read-story a, .new-home-blog-section .grid--blog .article__title .read-story a {
  font-family: "Tungsten";
  font-style: normal;
  font-weight: 500;
  font-size: 24px;
  line-height: 29px;
  text-transform: uppercase;
  margin: 0;
  position: relative;
}
 
.another-blog-list .article__title .read-story a:after, .new-home-blog-section .grid--blog .article__title .read-story a:after{
  content: "";
  position: absolute;
  top: 54%;
  background: url('https://cdn.shopify.com/s/files/1/0573/2201/2880/t/56/assets/arrow-bo-right.png?v=1664285703');
  width: 50px;
  height: 10px;
  left: auto;
  background-size: 100%;
  background-repeat: no-repeat;
  right: -75px;
  transform: translateY(-50%);
} 

.new-home-blog-section .blog-list-view.blog-list{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-top: 80px;
} 
.new-home-blog-section .blog-list-view.blog-list .home-blog{
  width: calc(50% - 25px);
}
.new-home-blog-section .grid--blog > .grid__item{
  width: calc(50% - 25px);
}
.new-home-blog-section .grid--blog > .grid__item.first-grid{
  width: 100%;
  margin-bottom: 28px;
}
.new-home-blog-section .grid--blog{
  margin-left: 0;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.new-home-blog-section .grid--blog:after{
  display: none;
}  
.new-home-blog-section .grid--blog .grid__item{
 padding-left: 0; 
}
.new-home-blog-section .blog-list-view.blog-list .home-blog .article-listing{
  display: flex;
  flex-direction: column-reverse;
}
.view-all-btn.hide-this{
  display: none;
}
@media screen and (max-width: 1440px) {
  .other-grid
    .page-width
    .grid
