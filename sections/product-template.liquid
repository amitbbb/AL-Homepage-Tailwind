      <li data-var-title="{{ variant.title | replace: ' ','-' | replace: '/','' | replace: '--','-'  | replace: ':', '' | replace: '"', '' | downcase }}" class="active"><a href="#tab-{{ind}}-{{ forloop.index }}">Details</a></li>
      		{% endif %}
      	{% endfor %}
      {% endcomment %}
      	{% endif %}
      </ul>
      {% endfor %}
    
