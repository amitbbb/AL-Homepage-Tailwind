      <li data-var-title="{{ variant.title | replace: ' ','-' | replace: '/','' | replace: '--','-'  | replace: ':', '' | replace: '"', '' | downcase }}"><a href="#tab-{{ind}}-{{ forloop.index }}a">Shipping &amp; Returns</a></li>
      {% comment %}
      	{% for jField in variant.metafields.jetti %}
