            </div>
            <div id="whymPlacement"></div>
          </div>

          <ul id="var-skus" style="display:none!important;">
            {% for variant in product.variants %}
            <li data-sku="{{ variant.sku }}" data-var="{{ variant.title | escape }}"></li>
            {% endfor %}
          </ul>
          {% comment %}<p>Item Number: <span data-currentsku>{{ current_variant.sku }}</span></p>{% endcomment %}
          {% comment %}
          <p class="not-avail">NOT AVAILABLE ONLINE</p>
          {% endcomment %}
        </div>
        {% endcomment %}
        {% if section.settings.show_share_buttons %}
          {% include 'social-sharing', share_title: product.title, share_permalink: product.url, share_image: product.featured_media %}
        {% endif %}
        <div class="registry-wrap">
          {% render 'aaa_giftregistry_btn' %}
        </div>
        <div class="wishlist">
          {% render 'ZooomyListWishlistProduct' %}
        </div>
      </div>

<div class="desc-tab">
  <header class="tabs-nav">
    <ul>
      <li class="active"><a href="#tab-1">Details</a></li>
      <li><a href="#tab-2">Shipping &amp; Returns</a></li>
    </ul>
  </header>
 
  <section class="tabs-content">
    <div id="tab-1">
    <div class="desc-text-wrap">
      <p><strong>Item Number</strong></p>
      <p><span data-currentsku>{{ current_variant.sku }}</span></p>
    </div>
      
      {% assign counter = 0 %}
      {% for jField in product.metafields.jetti %}
      {% assign jLabel = jField[0] | downcase %}
      	{% if jLabel == "designer"%}
      		{% assign counter = counter | plus: 1 %}
      <div class="desc-text-wrap {% if counter > 1 %}wrapDescText {% endif %}">
        <p><strong>{{ jField[0] | replace: '_', ' '}}</strong></p>
        <p>{{ jField[1] }}</p>
      </div>
      	{% endif %}
      {% endfor %}

      {% for jField in product.metafields.jetti %}
      {% assign jLabel = jField[0] | downcase %}
      	{% if jLabel != "return_policy" and jLabel != "available_to_ship" and jLabel != "shipping_dimensions_and_weight" and jLabel != "shipping_dimensions" and jLabel != "special_handling_fee" and jLabel != "designer" %}
      {% assign counter = counter | plus: 1 %}
      <div class="desc-text-wrap {{ jField[0] }} {% if counter > 1 %}wrapDescText{% endif %}">
        <p><strong>{{ jField[0] | replace: '_', ' '}}</strong></p>
        <p>{{ jField[1] }}</p>
      </div>
      {% endif %}
      {% endfor %}
      {%comment%}
      {% if counter > 1 %}
      <p class="desc-readmore">Read more</p>
      {% endif %}
      {%endcomment%}
    </div>
    <div id="tab-2">
      {% for jField in product.metafields.jetti %}
      {% assign jLabel = jField[0] | downcase %}
      {% if jLabel == "delivery & payment" or jLabel == "returnable" or jLabel == "weight" or jLabel == "return_policy" or jLabel == "available_to_ship" or jLabel == "shipping_dimensions" or jLabel == "special_handling_fee" or  jLabel == "shipping_dimensions_and_weight"%}
      <p class="tab-head"><strong>{{ jField[0] | replace: '_', ' ' | upcase }}</strong></p>
      <p class="tab-text">{{ jField[1] }}</p>
      {% endif %}
      {% endfor %}
    </div>
  </section>
</div>
      
      
      
      
      
{%comment%}      
<div class="desc-tab">
  <header class="tabs-nav">
      {% for variant in product.variants %}
    <ul data-ul-nav="{{ variant.title | replace: ' ','-' | replace: '/','' | replace: '--','-' | replace: ':', '' | replace: '"', '' | downcase }}">
        {% assign ind = forloop.index %}
      	{% if variant.metafields.jetti.size > 0 %}
