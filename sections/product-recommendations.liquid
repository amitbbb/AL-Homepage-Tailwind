{% comment %}
{%- if section.settings.show_product_recommendations -%}
  {%- if recommendations.performed -%}
    {%- if recommendations.products_count > 0 -%}
      <div class="main-recommendations product-recommendations__inner">
        {%- if section.settings.heading != blank -%}
          <div class="section-header">
            {% comment %}
            <h2>{{ section.settings.heading | escape }}</h2>
            {% endcomment %}
            <h2>SIMILAR PRODUCTS</h2>
          </div>
        {%- endif -%}
        <ul class="grid recommanded-list grid--uniform grid--view-items">
          {%- for product in recommendations.products limit:10 -%}
            <li class="grid__item small--one-half medium-up--one-third">
              {% include 'product-card-grid', max_height: 250, product: product, show_vendor: section.settings.show_vendor, recom:true %}
            </li>
          {%- endfor -%}
        </ul>
      </div>
    {%- endif -%}
  {%- else  -%}
    <div class="page-width recommanded-products fullWidth" data-base-url="{{ routes.product_recommendations_url }}" data-product-id="{{ product.id }}" data-section-id="{{ section.id }}" data-section-type="product-recommendations"></div>
  {%- endif -%}
{%- endif -%}
{% endcomment %}




